<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<!-- auto register Processor -->
	<context:annotation-config />
	<import resource="classpath:context/cxf.xml" />
	<import resource="classpath:context/um-service-bean.xml" />
	<import resource="classpath:context/context-service.xml" />
	<import resource="classpath:context/context-dao.xml" />
	<import resource="classpath:context/persistence-common.xml" />
    <import resource="classpath:META-INF/spring/spring-keyproperties.xml" />


</beans>
